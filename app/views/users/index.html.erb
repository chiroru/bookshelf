<h1>Listing users</h1>

<table>
  <tr>
     <th>ID</th>
     <th>Mail</th>
     <th>Edit</th>
     <th>Destroy</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id%></td>
    <td><%= user.email %></td>
    <td><%= link_to "edit", edit_user_registration_path(user.id) %></td>
    <td><%= link_to "destroy", user, method: :delete %></td>
  </tr>
  <% end %>
</table>

